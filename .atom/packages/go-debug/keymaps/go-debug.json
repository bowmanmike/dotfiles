{
	"atom-workspace": {
		"f5":            "go-debug:run-package",
		"ctrl-f5":       "go-debug:run-tests",
		"shift-f5":      "go-debug:restart",
		"f6":            "go-debug:stop",
		"f8":            "go-debug:continue",
		"f9":            "go-debug:toggle-breakpoint",
		"f10":           "go-debug:next",
		"f11":           "go-debug:step"
	},
	"body.platform-win32 atom-workspace, body.platform-linux atom-workspace": {
		"ctrl-k ctrl-g": "go-debug:toggle-panel"
	},
	"body.platform-darwin atom-workspace": {
		"cmd-k cmd-g": "go-debug:toggle-panel"
	}
}
