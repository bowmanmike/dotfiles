{
  "elixir.pathToElixirLS": "~/coding/elixir/elixir-ls/release/language_server.sh",
  "elixirLS.dialyzerEnabled": false,
  "elixirLS.fetchDeps": false,
  "elixirLS.suggestSpecs": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "ruby",
    "html",
    "css",
    "javascript",
    "terraform"
  ],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
